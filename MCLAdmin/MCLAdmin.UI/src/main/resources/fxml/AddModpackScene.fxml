<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="900.0" prefWidth="1600.0"
            xmlns="http://javafx.com/javafx/10.0.1"
            fx:controller="net.toastynetworks.MCLAdmin.UI.Controller.AddModpackController">
    <children>
        <TextField fx:id="nameTextField" layoutX="18.0" layoutY="56.0" prefHeight="25.0" prefWidth="203.0"
                   promptText="modpack name" AnchorPane.leftAnchor="20.0"/>
        <TextField fx:id="versionTextField" layoutX="20.0" layoutY="109.0" prefHeight="25.0" prefWidth="203.0"
                   promptText="modpack version" AnchorPane.leftAnchor="20.0"/>
        <Button fx:id="backButton" layoutX="462.0" layoutY="335.0" mnemonicParsing="false"
                onAction="#backToMainMenuButton" prefHeight="42.0" prefWidth="126.0" text="Back"
                AnchorPane.bottomAnchor="25.0" AnchorPane.rightAnchor="10.0"/>
        <Label layoutX="19.0" layoutY="31.0" text="Modpack name:" AnchorPane.leftAnchor="20.0"/>
        <Label layoutX="20.0" layoutY="90.0" text="Modpack version:" AnchorPane.leftAnchor="20.0"/>
        <Button fx:id="AddModpackButton" layoutX="325.0" layoutY="335.0" mnemonicParsing="false" onAction="#AddModpack"
                prefHeight="42.0" prefWidth="126.0" text="Add Modpack" AnchorPane.bottomAnchor="25.0"
                AnchorPane.rightAnchor="150.0"/>
    </children>
</AnchorPane>
